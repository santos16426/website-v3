{
    "overrides": [
      {
        "files": ["*.tsx"],
        "options": {
          "semi": false,
          "singleQuote": true,
          "tabWidth": 2,
          "printWidth": 80,
          "jsxSingleQuote": false,
          "trailingComma": "all",
          "bracketSpacing": true,
          "jsxBracketSameLine": false,
          "arrowParens": "always",
          "useTabs": false
        }
      }
    ]
  }
  